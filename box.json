
{
    "alias": "typehint-to-docblock.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": [
        "src"
    ],
    "extract": true,
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "tests", "Tests"
            ],
            "in": "vendor"
        }
    ],
    "git-commit": "git-commit",
    "git-version": "git-version",
    "main": "bin/typehint-to-docblock",
    "output": "typehint-to-docblock.phar",
    "stub": true
}